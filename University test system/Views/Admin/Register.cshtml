@model University_test_system.ViewModels.Account.RegisterViewModel
@{
    ViewData["Title"] = "Реєстрація студента";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="min-vh-100 d-flex align-items-center justify-content-center py-5"
     style="background: linear-gradient(135deg,#8B0A1E 0%,#C0122C 50%,#E8364F 100%);">

    <div class="container" style="max-width:500px">

        <div class="text-center mb-4">
            <div class="mx-auto mb-3 d-flex align-items-center justify-content-center"
                 style="width:72px;height:72px;border-radius:50%;overflow:hidden;border:2px solid rgba(255,255,255,.4)">
                <img src="data:image/png;base64,"
                     alt="КНУ" 
                     style="width:100%;height:100%;object-fit:cover">
            </div>
            <h4 style="font-family:'Playfair Display',serif;color:#fff;font-size:1.4rem;margin:0">
                КНУ ім. Шевченка
            </h4>
            <p style="color:rgba(255,255,255,.7);font-size:.85rem;margin:.25rem 0 0">
                Система тестування
            </p>
        </div>

        <div class="knu-card p-4 fade-in-up">
            <h5 class="section-title">Реєстрація студента</h5>

            <form asp-controller="Account" asp-action="Register" method="post">
                @Html.AntiForgeryToken()
                <div asp-validation-summary="ModelOnly" class="alert alert-knu-danger mb-3"></div>

                <div class="mb-3">
                    <label asp-for="DisplayName" class="form-label">
                        <i class="bi bi-person me-1 text-knu"></i>Ім'я користувача
                    </label>
                    <input asp-for="DisplayName" class="form-control" placeholder="student_knu" autocomplete="username" />
                    <span asp-validation-for="DisplayName" class="text-danger small"></span>
                </div>

                <div class="mb-3">
                    <label asp-for="Email" class="form-label">
                        <i class="bi bi-envelope me-1 text-knu"></i>Email
                    </label>
                    <input asp-for="Email" class="form-control" placeholder="users@email.com" autocomplete="email" />
                    <span asp-validation-for="Email" class="text-danger small"></span>
                </div>

                <div class="mb-3">
                    <label asp-for="Password" class="form-label">
                        <i class="bi bi-lock me-1 text-knu"></i>Пароль
                    </label>
                    <div class="input-group">
                        <input asp-for="Password" type="password" class="form-control"
                               placeholder="Мін. 6 символів" id="pwdField" autocomplete="new-password" />
                        <button class="btn btn-outline-secondary" type="button"
                                onclick="togglePassword('pwdField',this)">
                            <i class="bi bi-eye"></i>
                        </button>
                    </div>
                    <span asp-validation-for="Password" class="text-danger small"></span>
                </div>

                <div class="mb-4">
                    <label asp-for="ConfirmPassword" class="form-label">
                        <i class="bi bi-lock-fill me-1 text-knu"></i>Підтвердити пароль
                    </label>
                    <div class="input-group">
                        <input asp-for="ConfirmPassword" type="password" class="form-control"
                               placeholder="Повторіть пароль" id="pwdConfirmField" autocomplete="new-password" />
                        <button class="btn btn-outline-secondary" type="button"
                                onclick="togglePassword('pwdConfirmField',this)">
                            <i class="bi bi-eye"></i>
                        </button>
                    </div>
                    <span asp-validation-for="ConfirmPassword" class="text-danger small"></span>
                </div>

                <button type="submit" class="btn btn-knu w-100 py-2 fs-6">
                    <i class="bi bi-person-plus me-1"></i>Додати студента
                </button>
            </form>

            <hr class="divider-knu my-4" />

            <p class="text-center text-muted small mb-0">
                Вже є акаунт?
                <a href="/Account/Login" class="text-knu fw-600 text-decoration-none">Увійти</a>
            </p>
        </div>
    </div>
</div>

@section Scripts {
<script>
    function togglePassword(id, btn) {
        var f = document.getElementById(id);
        var isPass = f.type === 'password';
        f.type = isPass ? 'text' : 'password';
        btn.querySelector('i').className = isPass ? 'bi bi-eye-slash' : 'bi bi-eye';
    }
</script>
}
